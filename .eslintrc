{
  "globals": {
    "_": false,
    "__dirname": false,
    "require": false,
    "localStorage": false,
    "setTimeout": false,
    "Promise": false,
    "console": false,
    "chrome": false,
    "angular": false,
    "document": false,
    "process": false,
    "inject": false,
    "module": false,
    "window": false
  },
  "parser": "babel-eslint",
  "rules": {
    "brace-style": [
      2,
      "1tbs",
      { "allowSingleLine": false }
    ],
    "camelcase": 1,
    "comma-dangle": [ 1, "never" ],
    "curly": 0,
    "dot-notation": 1,
    "eqeqeq": 2,
    "indent": [
      1,
      4,
      { "SwitchCase": 1 }
    ],
    "new-parens": 1,
    "no-bitwise": 1,
    "no-cond-assign": 1,
    "no-debugger": 2,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-empty": 1,
    "no-invalid-regexp": 1,
    "no-invalid-this": 1,
    "no-mixed-spaces-and-tabs": [ 1, "smart-tabs" ],
    "no-multiple-empty-lines": [
      1,
      { "max": 2 }
    ],
    "no-undef": 1,
    "no-underscore-dangle": 1,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "one-var": [ 1, "never" ],
    "quote-props": [ 1, "as-needed" ],
    "semi": [ 2, "always" ],
    "keyword-spacing": 1,
    "space-unary-ops": [
      1,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "strict": [ 1, "function" ],
    "vars-on-top": 1,
    "wrap-iife": [ 1, "outside" ],
    "yoda": [ 1, "never" ],
    "require-jsdoc": [
      "error",
      {
        "require": {
          "FunctionDeclaration": false,
          "MethodDefinition": true,
          "ClassDeclaration": false
        }
      }
    ],

    //ES6 Stuff
    "arrow-parens": 0,
    "arrow-spacing": 1,
    "constructor-super": 1,
    "no-class-assign": 1,
    "no-const-assign": 1,
    "no-dupe-class-members": 1,
    "no-this-before-super": 1,
    "no-var": 2,
    "object-shorthand": 1,
    "prefer-arrow-callback": 1,
    "prefer-const": 1
  },
  "env": {
    "jasmine": true
  }
}